<div class ="container" style="margin-top:30px">
  <form action="/home/complete" method="POST" enctype="multipart/form-data" >
    <div class="form-group">
      <input type = "hidden" value = "<%= current_user.email %>" name ="user_email">
      <input type="text" class="form-control" name="title" id="title_id" placeholder="제목을 입력하세요">
    </div>
    <div class="form-group">
      <textarea class="form-control" rows="5" name="content" id="content_id" placeholder="내용을 입력하세요"></textarea>
    </div>
    <div class="form-group">
      <label for="exampleContact">Contact</label>
      <input type="text" class="form-control" name="contact" id="contact_id" placeholder="연락처를 입력하세요">
    </div>
    <div class="form-group">
      <label for="exampleInputFile" style="margin-right:40px">사진 첨부하기</label>
        <div class="file-field input-field">
          <div class="btn waves-effect waves-light" style="background-color:#9e9e9e">
            <span>사진첨부</span>
            <input type="file" name = "pic" id="exampleInputFile">
          </div>
          <div class="file-path-wrapper">
            <input class="file-path validate" type="text">
          </div>
        </div>
        
        
   <!--   <input type="file" name = "pic" id="exampleInputFile">-->
    </div>
    <button class="btn waves-effect waves-light" style="background-color:#9e9e9e; margin-top:40px" type="submit">글쓰기!</button>
  </form>
</div>